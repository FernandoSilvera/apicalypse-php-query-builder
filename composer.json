{
    "name": "apicalypse/php-query-builder",
    "description": "PHP query builder for Apicalypse API [IGDB].",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Silvera",
            "email": "fernando.silvera02@gmail.com"
        }
    ],
    "keywords": [
        "apicalypse",
        "query-builder",
        "php",
        "igdb",
        "query",
        "builder",
        "api"
    ],
    "homepage": "https://github.com/FernandoSilvera/apicalypse-php-query-builder",
    "support": {
        "issues": "https://github.com/FernandoSilvera/apicalypse-php-query-builder/issues",
        "source": "https://github.com/FernandoSilvera/apicalypse-php-query-builder"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Apicalypse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apicalypse\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            "/.github",
            "/phpunit.xml"
        ]
    }
}
